{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "outDir": "dist",
    "declaration": true,
    "lib": [
      "es6"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmit": false,
    "strict": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
